= form_for @usage do |f|
  - if @usage.errors.any?
    #error_explanation
      %h2= "#{pluralize(@usage.errors.count, "error")} prohibited this usage from being saved:"
      %ul
        - @usage.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :filePath
    = f.text_field :filePath
  .field
    = f.label :commandUsage
    = f.text_field :commandUsage
  .actions
    = f.submit 'Save'
